[all]
# <target hostname1> ansible_host=xx.xx.xx.xx (ip of your target host) hostname_dataplane=<hostname of your data plane 1> dataplane_ip=<xx.xx.xx.xx> (ip of your dataplane)
# an example line might look like this:
dell100g ansible_host=172.16.248.237 hostname_dataplane=smicro-unten.labor2.bisdn.de dataplane_ip=172.16.248.172
control-plane-1 ansible_host=172.16.248.137 hostname_dataplane=smicro-unten.labor2.bisdn.de dataplane_ip=172.16.248.136

# to read more about ansible inventories, please check to official docs available here: https://docs.ansible.com/ansible/latest/user_guide/intro_inventory.html
